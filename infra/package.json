{
  "name": "@alliance-risk/infra",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "diff": "cdk diff",
    "test": "jest",
    "cfn:synth": "cdk synth --no-staging > cfn/alliance-risk-stack.template.yaml",
    "cfn:validate": "aws cloudformation validate-template --template-body file://cfn/alliance-risk-stack.template.yaml",
    "cfn:deploy": "bash cfn/deploy.sh"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.177.0",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "aws-cdk": "^2.177.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
